{
  "content_scripts": [
    {
      "matches": [
        "*://web.whatsapp.com/*"
      ], 
      "js": [
        "contentscript.js"
      ]
    }
  ], 
  "browser_action": {
    "default_icon": "icons/whatsapp_128.png", 
    "default_title": "WhatsApp Launch"
  }, 
  "description": "Fast Launcher with WhatsApp Launch, you'll get fast, simple, secure messaging.", 
  "icons": {
    "128": "icons/whatsapp_128.png", 
    "64": "icons/whatsapp_64.png", 
    "48": "icons/whatsapp_48.png", 
    "16": "icons/whatsapp_16.png"
  }, 
  "background": {
    "scripts": [
      "background.js"
    ]
  }, 
  "name": "WhatsApp Launch", 
  "options_ui": {
    "chrome_style": true, 
    "page": "options.html", 
    "open_in_tab": false
  }, 
  "version": "1.2.3", 
  "options_page": "options.html", 
  "manifest_version": 2, 
  "permissions": [
    "tabs", 
    "https://web.whatsapp.com/", 
    "storage"
  ]
}